{
	"manifest_version": 2,
	"name": "Productivity App",
	"description": "Helps you measure where you spend time on the internet",
	"version": "1.2.5",
	"key": "",
	"browser_action": {    
		"default_popup_cc": "index.html"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"icons": {
		"16": "./src/images/icon16.png",
		"48": "./src/images/icon48.png",
		"128": "./src/images/icon128.png"
	},
	"permissions": [
		"background",
		"tabs",
		"storage",
		"idle",
		"identity",
    "https://accounts.google.com/*",
    "https://www.googleapis.com/*"
	],
	"oauth2": {
    "client_id": "797463233889-2hav69qskd3mcv20hm3s8ptfcko5mvo6.apps.googleusercontent.com",    							
    "scopes": ["https://www.googleapis.com/auth/plus.login"]
  }
}